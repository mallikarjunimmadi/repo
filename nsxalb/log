# Show the detected entity_uuid for each JSON we saved
for f in /tools_data/reports/vsMetrics/*.json; do
  printf "%s -> " "$(basename "$f")"
  jq -r '.series[0].header.entity_uuid // "NO_SERIES"' "$f"
done

# Count by prefix (VS/POOL/SE)
for f in /tools_data/reports/vsMetrics/*.json; do
  jq -r '.series[0].header.entity_uuid // "NO_SERIES"' "$f"
done | awk '
/^virtualservice-/ {vs++}
/^pool-/           {pl++}
/^serviceengine-/  {se++}
/^NO_SERIES/       {ns++}
END {printf "VS=%d Pool=%d SE=%d NoSeries=%d\n", vs, pl, se, ns}'
